{
  "family": "theater-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::095808160027:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "theater-api",
      "image": "095808160027.dkr.ecr.us-east-2.amazonaws.com/theater-api:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
